{
    "name": "krinzz/php-unit-test",
    "require-dev": {
        "phpunit/phpunit": "^10.3"
    },
    "authors": [
        {
            "name": "Akmal-Keisin",
            "email": "akmalkeisin@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AkmalKeisin\\Test\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AkmalKeisin\\Test\\": "tests"
        }
    },
    "require": {},
    "scripts": {
        "test": "vendor/bin/phpunit --configuration phpunit.xml",
        "test:mock": "vendor/bin/phpunit --configuration phpunit.xml --testsuite mock"
    }
}
